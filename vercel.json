{
  "builds":[
    {
      "src":"./index.js",
      "use":"@vercel/node"
    },
    {
      "src":"./speech/requirements.txt",
      "use":"@vercel/python",
      "env": {
        "PORTAUDIO": "portaudio19-dev"
      }
    }
  ],
  "routes":[
    {
      "src":"/.*",
      "dest":"index.js"
    }
  ]
}
